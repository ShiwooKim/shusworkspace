{
  "name": "shusworkspace",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:workers": "pnpm dlx wrangler@3 deploy cloudflare-workers/auth-worker.js --config cloudflare-workers/wrangler.toml",
    "deploy:workers:secure": "pnpm dlx wrangler@3 deploy cloudflare-workers/auth-worker-secure.js --config cloudflare-workers/wrangler.toml",
    "deploy:workers:simple": "pnpm dlx wrangler@3 deploy cloudflare-workers/auth-worker-simple.js --config cloudflare-workers/wrangler.toml",
    "deploy:all": "pnpm build && docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
